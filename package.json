{
  "name": "boilerplate-mobx",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "stub": "node stub",
    "start": "webpack-dev-server --hot --inline --history-api-fallback --host 0.0.0.0 & npm run stub",
    "build": "webpack -p --progress --config webpack.prod.config.js",
    "lint": "eslint ./src",
    "test": "NODE_PATH=$(pwd)/src jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-jest": "^19.0.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.2.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "hapi": "^16.1.1",
    "ignore-styles": "^5.0.1",
    "jest": "^20.0.1",
    "joi": "^10.2.2",
    "jsdom": "^9.8.3",
    "nyc": "^10.0.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.0.2",
    "react-test-renderer": "^15.3.2",
    "sinon": "^2.0.0-pre.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "axios-mock-adapter": "^1.8.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react": "^1.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.23.1",
    "draft-js": "^0.10.1",
    "file-loader": "^0.9.0",
    "formsy-react": "^0.18.1",
    "html-webpack-plugin": "^2.22.0",
    "js-cookie": "^2.1.3",
    "mobx": "^2.6.2",
    "mobx-react": "^3.5.3",
    "moment": "^2.13.0",
    "moment-range": "^2.2.0",
    "node-sass": "^4.5.3",
    "numbro": "^1.11.0",
    "rc-progress": "^2.0.2",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-datepicker": "^0.28.2",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.3",
    "react-maskedinput": "^3.3.0",
    "react-router": "^2.4.1",
    "react-slick": "^0.14.5",
    "rxjs": "^5.0.0-rc.2",
    "sass-loader": "^3.2.3",
    "slick-carousel": "^1.6.0",
    "string-mask": "^0.3.0",
    "style-loader": "^0.13.1",
    "styled-components": "^2.0.1",
    "webpack": "2.2.0"
  },
  "jest": {
    "testURL": "http://mobx.com",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/core/unit-test",
      "actions.js"
    ],
    "setupFiles": [
      "<rootDir>/src/core/unit-test/setup.js"
    ],
    "testRegex": "/__tests__/.*.spec.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg|png)$": "<rootDir>/src/core/unit-test/jest-stub.js"
    },
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
